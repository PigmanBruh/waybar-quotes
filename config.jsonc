{
    "layer": "bottom",
    "position": "top",
    "spacing": 4,
    "exclusive": false,
    "modules-left": [],
    "modules-center": ["custom/quote"],
    "modules-right": [],

    "custom/quote": {
        "exec": "cat ~/.waybar-quotes/picked.txt",
        "tooltip": false,
        "on-click": "$(cat ~/.waybar-quotes/dir.txt)/quote_picker.sh",
        "signal": 1
    }
}
